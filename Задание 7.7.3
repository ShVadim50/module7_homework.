const users = [
    { name: "Petr", age: 15 },
    { name: "Anna", age: 18 },
    { name: "Sveta", age: 32 },
]
const usersAgeMore18 = users.filter(user => {
    return user.age >= 18;
})

const nameOfUsers = users.map(user => {
    return user.name;
})

console.log(usersAgeMore18);
console.log(nameOfUsers);